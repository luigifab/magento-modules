<config>
	<modules>
		<Luigifab_Modules>
			<version>1.0.0</version>
		</Luigifab_Modules>
	</modules>
	<global>
		<blocks>
			<modules>
				<class>Luigifab_Modules_Block</class>
			</modules>
		</blocks>
		<helpers>
			<modules>
				<class>Luigifab_Modules_Helper</class>
			</modules>
		</helpers>
	</global>
	<adminhtml>
		<translate>
			<modules>
				<Luigifab_Modules>
					<files>
						<default>Luigifab_Modules.csv</default>
					</files>
				</Luigifab_Modules>
			</modules>
		</translate>
	</adminhtml>
</config>