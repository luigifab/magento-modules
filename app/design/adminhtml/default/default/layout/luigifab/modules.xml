<layout version="0.1.0">
	<adminhtml_modules_lists_index>
		<reference name="head">
			<action method="setTitle" translate="title">
				<title>Modules list / Tools / Magento Admin</title>
			</action>
			<action method="addItem">
				<type>skin_css</type>
				<name>css/luigifab/modules/styles.css</name>
